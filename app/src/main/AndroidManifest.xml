<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shuweizhao.wokba">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.IntermediateActivity">
        </activity>
        <activity android:name=".Activity.SignUpActivity">
        </activity>
        <activity android:name=".Activity.LogInActivity">
        </activity>
        <activity android:name=".Activity.MainUIActicity">
        </activity>
        <activity android:name=".Activity.StoreDetail">
        </activity>
        <activity android:name=".Activity.ShakeActivity">
        </activity>
        <activity android:name=".Activity.StoreListActivity">
        </activity>
        <activity android:name=".Activity.PlateOrderActivity">
        </activity>
        <activity android:name=".Activity.CheckOutActivity">
        </activity>
        <activity android:name=".Activity.FavoriteActivity">
        </activity>
        <activity android:name=".Activity.BindCardActivity">
        </activity>
        <activity android:name=".Activity.OptionActivity">
        </activity>
        <activity android:name=".Activity.OrderSuccessActivity">
        </activity>
    </application>

</manifest>
